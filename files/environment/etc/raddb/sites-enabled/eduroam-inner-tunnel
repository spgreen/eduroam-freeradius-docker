server eduroam-inner-tunnel {
 
authorize {
        auth_log
        eap
        files
        mschap
        pap
}
 
authenticate {
        Auth-Type PAP {
                pap
        }
        Auth-Type MS-CHAP {
                mschap
        }
        eap
}
 
post-auth {
        reply_log
        Post-Auth-Type REJECT {
                reply_log
        }
 
}
}        
